{
  "name": "@rxliuli/vista",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/vista.git"
  },
  "homepage": "https://github.com/rxliuli/vista",
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "pnpm build",
    "build": "tsup src/index.ts src/middlewares/*/index.ts --format=esm --dts",
    "dev": "pnpm build --watch",
    "test:server": "vite-node src/test-server.ts",
    "test": "vitest run"
  },
  "sideEffects": false,
  "devDependencies": {
    "@hono/node-server": "^1.13.7",
    "@vitest/browser": "^2.1.8",
    "hono": "^4.6.15",
    "playwright": "^1.49.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite-node": "^2.1.8",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
